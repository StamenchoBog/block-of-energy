    "@hyperledger/fabric-gateway": "^1.10.0",
    "mongodb": "^6.21.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "~18.19.130",
    "azure-functions-core-tools": "^4.6.0",
    "rimraf": "^5.0.10",
    "typescript": "^4.9.5"
  },
  "main": "dist/src/functions/continuousTemperAuditor.js"
}